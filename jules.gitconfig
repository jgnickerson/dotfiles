[user]
	name = Jules Nickerson
	email = jules.nickerson@liveramp.com
[alias]
	co = checkout
	pr = pull --rebase
	cm = commit -m
	ca = commit --amend
	col = checkout @{-1}
	copr = "!f() { git fetch origin refs/pull/${1?Missing PR}/head && git checkout FETCH_HEAD; }; f"
[push]
	default = simple
[hub]
	host = git.liveramp.net
[core]
	excludesfile = ~/.gitignore
[pager]
	branch = false
	log = true
